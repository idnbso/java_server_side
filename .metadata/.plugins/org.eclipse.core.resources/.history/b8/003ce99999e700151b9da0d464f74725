package com.github.idnbso.snippetodo;

public class Item
{
    private int id;
    private String description;
        
    /**
     * 
     */
    public Item()
    {
        super();
    }

    /**
     * @param id
     * @param description
     */
    public Item(int id, String description)
    {
        this.id = id;
        this.description = description;
    }
    
    
}
